{
  "name": "n8n-nodes-codetether",
  "version": "0.1.0",
  "description": "n8n community node for CodeTether AI task automation",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "codetether",
    "ai",
    "automation",
    "agent"
  ],
  "license": "MIT",
  "homepage": "https://codetether.io",
  "repository": {
    "type": "git",
