# Local development overrides
# This prevents NextAuth from redirecting back to a different app running on :3000
NEXTAUTH_URL=https://192.168.50.101:3001

# Keycloak auth method (client_secret_post sends credentials in body)
KEYCLOAK_PUBLIC_CLIENT=false
# KEYCLOAK_TOKEN_ENDPOINT_AUTH_METHOD=client_secret_basic

# Optional: point the dashboard to your API (defaults to https://api.codetether.run if unset)
# Proxied via Next.js rewrites to avoid mixed-content errors (HTTPS site → HTTP backend)
NEXT_PUBLIC_API_URL=/api
# Cognition API is now proxied via Next.js rewrites — see next.config.js
# The backend URL is set via COGNITION_API_BACKEND (server-side only, no NEXT_PUBLIC_)
COGNITION_API_BACKEND=http://192.168.50.101:8010

# dev mode
DEV_MODE=true

# Enable NextAuth debug mode
AUTH_DEBUG=true

# Keycloak client secret (override shell env var)
KEYCLOAK_CLIENT_SECRET=Boog6oMQhr6dlF5tebfQ2FuLMhAOU4i1

# Server-side API URL (auth callbacks need absolute URLs)
API_URL=http://192.168.50.101:8001
